{
  "expo": {
    "name": "MEMMIE",
    "slug": "MEMMIE",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "com.develite.memmie",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.MEMMIE",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Allow access to your current location for using on the map",
        "NSLocationAlwaysUsageDescription": "Allow access to your current location for using on the map",
        "NSCameraUsageDescription": "Allow access to your photo library to select images for uploading."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["android.permission.RECORD_AUDIO"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "14.0"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "7ec00801-23b5-40a8-bd93-ee37d9a9ae43"
      }
    }
  }
}
