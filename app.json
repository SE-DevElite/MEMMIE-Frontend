{
  "expo": {
    "name": "MEMMIE",
    "slug": "memmie",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "com.develite.memmie",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.MEMMIE",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Allow access to your current location for using on the map",
        "NSLocationAlwaysUsageDescription": "Allow access to your current location for using on the map",
        "NSCameraUsageDescription": "Allow access to your photo library to select images for uploading."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.anonymous.MEMMIE"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./local/assets/notification-icon.png",
          "color": "#ffffff"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "13f91eee-4288-4d46-8710-0c3bccbaac88"
      }
    },
    "owner": "punnapathianchai"
  }
}
